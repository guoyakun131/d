/**index.wxss**/
.top{
  /* padding: 40rpx 0; */
  background-image: url(data:image/png;base64,);
  background-size: cover;
}
.top image{
  width: 100%;
  height: 190rpx;
}
.tab-nav {
  justify-content: space-around;
  align-items: center;
}
.tab-nav .item{
  text-align: center;
  font-size: 12px;
  width: 33.333%;
  border-bottom: 2px solid transparent;
}
.tab-nav .item .number{
  font-size: 16px;
  font-weight: bold;
  color: #333;
}
.tab-nav .item .title{
  color: #999;
}
.tab-nav .item.active{
  border-bottom: 2px solid #ff4444;
}
.tab-nav .item.active .number,.tab-nav .item.active .title{
  color: #ff4444;
}
.tab-box .item{
  margin-top: 10rpx;
  width: 50%;
  float: left;
  padding: 0 5rpx;
}
.tab-box .item:nth-child(2n){
  padding-right: 0;
}
.tab-box .item:nth-child(2n+1){
  padding-left: 0;
}
.tab-box .item image{
  width: 100%;
  height: 370rpx;
}
.tab-box .item .name{
  display: -webkit-box;
  word-break: break-all;
  overflow: hidden; 
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.tab-box .item .price{
  margin-top: 10rpx;
  color: #ff4444;
  font-size: 16px;
}
.tab-box .info{
  padding: 10rpx 22rpx 25rpx 22rpx;
  background: #fff;
}
.refer-box{
  position: fixed;
  bottom: 100rpx;
  right: 22rpx;
  text-align: center;
  color: #ff8f8f;
}
.refer-box button{
  background-color: #ff8f8f;
  border-radius: 50%;
  width: 95rpx;
  height: 95rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0;
  margin: 0;
  box-shadow: 0 0 5px #ff8f8f;
}
.refer-box button view{
  width: 48rpx;
  height: 48rpx;
  background-image: url(data:image/png;base64,);
  background-size: cover;
}
.no-shop .img{
  width: 141rpx;
  height: 124rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
  margin: 150rpx auto 40rpx auto;
}
.no-shop .txt{
  color: #999;
  font-size: 16px;
  text-align: center;
}